        --><span class="toggle-icon"><i class="la la-bars"></i></span><!--
    --></button><!--
    --><ul id="nav-left" class="nav-bar"><!--
        --><% for (let item of ctx.items) { %><!--
            --><% if (item.direction === "left") { %><!--
